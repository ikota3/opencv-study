   "source": [
    "def onTrackbar(position):\n",
    "    global trackValue\n",
    "    trackValue = position\n",
    "\n",
    "trackValue = 100\n",
    "cv2.namedWindow('img')\n",
    "cv2.createTrackbar('trackbar', 'img', trackValue, 255, onTrackbar)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
  }
